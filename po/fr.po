msgstr " Une seule option parmi -c -d -t -l -C, -x ou -+ est permise"

#: src/dar_suite/command_line.cpp:808
msgid "Only one -A option is allowed"
msgstr "Une seule option -A est permise"

#: src/dar_suite/command_line.cpp:835
msgid "Error while parsing -A argument as a date: "
msgstr "Erreur de syntaxe dans la date donné à l'option -A : "

#: src/dar_suite/command_line.cpp:900
msgid "Choose only one compression algorithm"
msgstr "Choisir un seul algorithme de compression"

#: src/dar_suite/command_line.cpp:906
msgid "-w option is useless with -n"
msgstr "-w est inutile avec -n"

#: src/dar_suite/command_line.cpp:918
msgid "Unknown argument given to -w: "
msgstr "Argument inconnu donné à -w : "

#: src/dar_suite/command_line.cpp:938 src/dar_suite/command_line.cpp:945
#: src/dar_suite/command_line.cpp:952
