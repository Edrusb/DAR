#: src/dar_suite/command_line.cpp:2204
#, c-format
msgid "Usage:\n"
msgstr "Usage :\n"

#: src/dar_suite/command_line.cpp:2205
#, c-format
msgid ""
"  %s [ -c | -t | -l | -x | -d | -+ | -C | -y ] [<path>/]<basename> "
"[options]\n"
msgstr ""
"  %s [ -c | -t | -l | -x | -d | -+ | -C | -y ] [<path>/]<basename> "
"[options...]\n"

#: src/dar_suite/command_line.cpp:2209
#, c-format
msgid "Commands are:\n"
msgstr "Les commandes sont :\n"

#: src/dar_suite/command_line.cpp:2210
#, c-format
msgid "   -c  create an archive\n"
msgstr "   -c  créer une archive\n"

#: src/dar_suite/command_line.cpp:2211
#, c-format
msgid "   -t  test the archive integrity\n"
msgstr "   -t  test l'intégrité d'une archive\n"

#: src/dar_suite/command_line.cpp:2212
#, c-format
msgid "   -l  list the contents of the archive\n"
msgstr "   -l  affichage du contenu d'une archive\n"

#: src/dar_suite/command_line.cpp:2213
#, c-format
msgid "   -x  extract files from the archive\n"
msgstr "   -x  restauration de fichiers\n"

#: src/dar_suite/command_line.cpp:2214
#, c-format
msgid "   -d  compare the archive with the existing filesystem\n"
msgstr "   -d  comparaison de l'archive avec le fichiers existants\n"

#: src/dar_suite/command_line.cpp:2215
#, c-format
msgid "   -+  merge two archives / create a sub archive\n"
msgstr "   -+  fusion de deux archive / création de sous-archive\n"

#: src/dar_suite/command_line.cpp:2216
#, c-format
msgid "   -C  isolate the catalogue from an archive\n"
msgstr "   -C  isole le catalogue d'une archive\n"

#: src/dar_suite/command_line.cpp:2217
#, c-format
msgid "   -y  repair a truncated archive\n"
msgstr "   -y  répare une archive tronquée\n"

#: src/dar_suite/command_line.cpp:2219
#, c-format
msgid "   -h  display this help information\n"
msgstr "   -h  affiche cette page d'aide\n"

#: src/dar_suite/command_line.cpp:2220
#, c-format
msgid "   -V  display version information\n"
msgstr "   -V  affiche les informations de version\n"

#: src/dar_suite/command_line.cpp:2223
#, c-format
msgid "   -v[s|t|d|m|f|a] verbose output\n"
msgstr "   -v[s|t|d|m|f|a] affichage détaillé\n"

#: src/dar_suite/command_line.cpp:2224
#, c-format
msgid "   -q              suppress final statistics report\n"
msgstr "   -q              supprime le rapport final de statistiques\n"

#: src/dar_suite/command_line.cpp:2225
#, c-format
msgid ""
"   -qcrypto        suppress initial warning when reading ciphered archive\n"
msgstr ""
"   -qcrypto        supprime l'avertissement initial lors de la lecture d'une "
"archive chiffrée\n"

#: src/dar_suite/command_line.cpp:2226
#, c-format
msgid ""
"   -Q              suppress the initial warning when not launched from a "
"tty\n"
msgstr ""
"   -Q              supprime le message initial si non lancé depuis un tty\n"

#: src/dar_suite/command_line.cpp:2227
#, c-format
msgid ""
"   -w              don't warn before overwriting slices or files to restore\n"
msgstr "   -w              ne pas avertir avant un écrasement de fichier\n"

#: src/dar_suite/command_line.cpp:2228
#, c-format
msgid "   -wa             don't warn before overwriting and removing files\n"
msgstr ""
"   -wa             ne pas avertir ni avant écrasement ni avant suppression "
"de fichier\n"

#: src/dar_suite/command_line.cpp:2229
#, c-format
msgid "   -n              don't overwrite slices\n"
msgstr "   -n              ne pas écraser de fichier\n"

#: src/dar_suite/command_line.cpp:2230
#, c-format
msgid "   -/ <policy>     define an overwriting policy\n"
msgstr "   -/ <politique>  definit une politique d'écrasement\n"

#: src/dar_suite/command_line.cpp:2231
#, c-format
msgid ""
"   -b              ring the terminal bell when user action is required\n"
msgstr ""
"   -b              fait sonner le terminal pour solliciter l'utilisateur\n"

#: src/dar_suite/command_line.cpp:2233
#, c-format
msgid ""
"   -G <num>,<num>  number of threads for de/ciphering and de/compression\n"
msgstr ""
"-G <num>,<num>   nombre de threads pour dé/chiffreer et dé/compresser\n"

#: src/dar_suite/command_line.cpp:2234
#, c-format
msgid ""
"   -O[ignore-owner | mtime | inode-type] do not consider user and group\n"
"                   ownership\n"
msgstr ""
"   -O[ignore-owner | mtime | inode-type]  ne pas prendre en compte "
"certaines\n"
"                   propriété des fichiers\n"

#: src/dar_suite/command_line.cpp:2235
#, c-format
msgid "   -H [N]          ignore shift in dates of an exact number of hours\n"
msgstr ""
"   -H [N]           considère identiques des dates qui diffèrent d'un nombre "
"entier d'heures\n"

#: src/dar_suite/command_line.cpp:2236
#, c-format
msgid "   -E <string>     command to execute between slices\n"
msgstr "   -E <chaîne>     commande exécutée entre les tranches\n"

#: src/dar_suite/command_line.cpp:2237
#, c-format
msgid "   -F <string>     same as -E but for the archive of reference\n"
msgstr "   -F <chaîne>     comme -E mais pour l'archive de référence\n"

#: src/dar_suite/command_line.cpp:2238
#, c-format
msgid ""
"   -~ <string>     same as -E but for the auxiliary archive of reference\n"
msgstr ""
"   -~ <chaîne>     comme -E mais pour l'archive de référence auxiliaire\n"

#: src/dar_suite/command_line.cpp:2239
#, c-format
msgid "   -u <mask>       mask to ignore certain EA\n"
msgstr "   -u <masque>     masque d'inclusion d'Attributs Étendus\n"

#: src/dar_suite/command_line.cpp:2240
#, c-format
msgid "   -U <mask>       mask to allow certain EA\n"
msgstr "   -U <masque>     masque d'exclusion d'Attributs Étendus\n"

#: src/dar_suite/command_line.cpp:2241
#, c-format
msgid "   -K [<algo>:]<string>   use <string> as key to encrypt/decrypt\n"
msgstr ""
"   -K [<algo>:]<chaîne>   mot de passe à utiliser pour chiffrer/déchiffrer\n"

#: src/dar_suite/command_line.cpp:2242
#, c-format
msgid "   -J [<algo>:]<string>   same as -K but for the archive of reference\n"
msgstr "   -J [<algo>:]<chaîne>   comme -K mais pour l'archive de référence\n"

#: src/dar_suite/command_line.cpp:2243
#, c-format
msgid ""
"   -$ [<algo>:]<string>   same as -K but for the auxiliary archive of "
"reference\n"
msgstr ""
"   -$ [<algo>:]<chaîne>   comme -E mais pour l'archive de référence "
"auxiliaire\n"

#: src/dar_suite/command_line.cpp:2244
#, c-format
msgid "   -# <integer>    encryption block size\n"
msgstr "   -# <entier>   taille des blocs de chiffrement\n"

#: src/dar_suite/command_line.cpp:2245
#, c-format
msgid "   -* <integer>    same as -# but for archive of reference\n"
msgstr "   -* <entier>   comme -# mais pour l'archive de référence\n"

#: src/dar_suite/command_line.cpp:2246
#, c-format
msgid "   -B <filename>   read options from given file\n"
msgstr "   -B <fichier>   lit les options à partir du fichier\n"

#: src/dar_suite/command_line.cpp:2247
#, c-format
msgid ""
"   -N              do not read ~/.darrc nor /etc/darrc configuration file\n"
msgstr "   -N              ignore ~/.darrc et /etc/darrc s'ils sont présents\n"

#: src/dar_suite/command_line.cpp:2248
#, c-format
msgid "   -e              dry run, fake execution, nothing is produced\n"
msgstr "   -e              exécution à vide (rien n'est écrit sur le disque)\n"

#: src/dar_suite/command_line.cpp:2249
#, c-format
msgid ""
"   -aa             do not try to preserve atime of file open for reading.\n"
msgstr ""
"   -aa             ne cherche pas à conserver atime des fichiers ouverts en "
"lecture.\n"

#: src/dar_suite/command_line.cpp:2250
#, c-format
msgid "   -ac             do not try to preserve ctime (default behavior).\n"
msgstr ""
"   -ac             ne cherche pas à conserver ctime (comportement par "
"défaut).\n"

#: src/dar_suite/command_line.cpp:2252
#, c-format
msgid "Common file selection options:\n"
msgstr "Options communes de sélection des fichiers :\n"

#: src/dar_suite/command_line.cpp:2253
#, c-format
msgid "   -R <path>  filesystem root directory (current dir by default)\n"
msgstr ""
"   -R <chemin> répertoire à considérer comme racine (rep. courant par "
"défaut)\n"

#: src/dar_suite/command_line.cpp:2254
#, c-format
msgid ""
"   -X <mask>  file name expression to exclude from the operat. (none by "
"default)\n"
msgstr "   -X <masque> fichiers à exclure de l'opération (aucun par défault)\n"

#: src/dar_suite/command_line.cpp:2255
#, c-format
msgid ""
"   -I <mask>  file name expression to include in the operation (all by "
"default)\n"
msgstr "   -I <masque> fichiers à inclure dans l'opération (tous par défaut)\n"

#: src/dar_suite/command_line.cpp:2256
#, c-format
msgid "   -P <path>  file path expression to exclude from the operation\n"
msgstr "   -P <chemin> sous-répertoires à exclure de l'opération\n"

#: src/dar_suite/command_line.cpp:2257
#, c-format
msgid "   -g <path>  file path fixed string to include in the operation\n"
msgstr "   -g <chemin> sous-répertoire à inclure dans l'opération\n"

#: src/dar_suite/command_line.cpp:2258
#, c-format
msgid "   -am        set ordered mode for all filters\n"
msgstr "   -am        filtres en mode ordonné\n"

#: src/dar_suite/command_line.cpp:2259
#, c-format
msgid ""
"   -an        the masks expression that follow are now case insensitive\n"
msgstr ""
"   -an        dans les masques suivants, majuscules et minuscules sont "
"équivalents\n"

#: src/dar_suite/command_line.cpp:2260
#, c-format
msgid ""
"   -acase     the masks expressions that follow are now case sensitive\n"
msgstr ""
"   -acase     dans les masques qui suivent, majuscules et minuscules sont "
"différents\n"

#: src/dar_suite/command_line.cpp:2261
#, c-format
msgid ""
"   -ar        set the following masks expression to be regex expressions\n"
msgstr ""
"   -ar        bascule les masques suivants comme expression régulières\n"

#: src/dar_suite/command_line.cpp:2262
#, c-format
msgid ""
"   -ag        set the following masks expression to be glob expressions\n"
msgstr "   -ag        bascule les masques suivants comme expression glob\n"

#: src/dar_suite/command_line.cpp:2263
#, c-format
msgid ""
"   -[ <filename>[:<eols>]   filename contains a list of files to include\n"
msgstr ""
"   -[ <fichier>[:<eols>] fichier contenant une liste de fichiers à inclure\n"

#: src/dar_suite/command_line.cpp:2264
#, c-format
msgid ""
"   -] <filename>[:<eols>]   filename contains a list of files to exclude\n"
msgstr ""
"   -[ <fichier>[<:eols>] fichier contenant une liste de fichiers à inclure\n"

#: src/dar_suite/command_line.cpp:2265
#, c-format
msgid ""
"        where <eols> is an optional comma separated string to define the end "
"of\n"
"        line used in <filename>. <oels> defaults to \"\\n,\\r\\n\"\n"
msgstr ""
"        où <eols> est une liste séparée par des virgules définissant les "
"séquences de fin de ligne\n"
"        dans <fichier>. par défaut <eols> vaut \"\\n,\\r\\n\"\n"

#: src/dar_suite/command_line.cpp:2266
#, c-format
msgid ""
"   -5[EA name] exclude files from backup having the given Extended "
"Attribute\n"
msgstr ""
"   -5[nom d'EA] exclure des fichiers de la sauvegarde possédant cet Attribut "
"Etendu\n"

#: src/dar_suite/command_line.cpp:2267
